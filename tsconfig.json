{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "module": "esnext",
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "paths": {
      "src/*": ["src/*"],
      "react-native/*": ["node_modules/react-native/*"]
    }
  },
  "include": ["src"]
}
